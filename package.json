{
  "name": "minhtran-site",
  "version": "4.0.0",
  "private": true,
  "description": "Professional portfolio website built with Next.js and Tailwind CSS",
  "author": "Minh Tran",
  "license": "MIT",
  "scripts": {
    "dev": "NEXT_TURBOPACK_TRACING=1 next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint --no-cache",
    "lint:fix": "next lint --no-cache --fix",
    "lint:strict": "next lint --max-warnings 0",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format": "npm run prettier:fix && npm run lint:fix",
    "check": "npm run prettier && npm run lint:strict",
    "analyze": "cross-env ANALYZE=true next build",
    "clean": "rm -rf .next out dist",
    "preview": "next build && next start",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "npm@10.4.0",
  "dependencies": {
    "@ai-sdk/cohere": "^1.0.3",
    "@apollo/client": "^3.13.8",
    "@plaiceholder/next": "^3.0.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@umami/api-client": "^0.79.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^4.3.19",
    "axios": "^1.10.0",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "clsx": "^2.1.0",
    "daisyui": "^5.0.46",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "jsdom": "^26.1.0",
    "next": "15.4.3",
    "next-share": "^0.27.0",
    "plaiceholder": "^3.0.0",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "reading-time": "^1.5.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "sharp": "^0.32.6",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.2.1",
    "theme-change": "^2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@next/bundle-analyzer": "^15.4.2",
    "@next/eslint-plugin-next": "^15.4.2",
    "@tailwindcss/typography": "^0.0.0-insiders.0339c42",
    "autoprefixer": "^10.4.17",
    "conventional-changelog-atom": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": "eslint --fix",
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
